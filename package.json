{
  "name": "refui",
  "version": "0.8.0",
  "description": "The JavaScript framework that refuels your UI projects, across web, native, and embedded",
  "main": "src/index.js",
  "type": "module",
  "types": "src/index.d.ts",
  "typesVersions": {
    "*": {
      "renderers/*": [
        "./src/renderers/*.d.ts"
      ],
      "presets/*": [
        "./src/presets/*.d.ts"
      ],
      "extras/*": [
        "./src/extras/*.d.ts"
      ]
    }
  },
  "scripts": {},
  "keywords": [
    "refui",
    "refuel",
    "ef",
    "framework",
    "frontend",
    "native",
    "embedded",
    "signal",
    "dom",
    "ui"
  ],
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "default": "./src/index.js"
    },
    "./hmr": {
      "types": "./src/hmr.d.ts",
      "import": "./src/hmr.js",
      "default": "./src/hmr.js"
    },
    "./signal": {
      "types": "./src/signal.d.ts",
      "import": "./src/signal.js",
      "default": "./src/signal.js"
    },
    "./renderer": {
      "types": "./src/renderer.d.ts",
      "import": "./src/renderer.js",
      "default": "./src/renderer.js"
    },
    "./components": {
      "types": "./src/components.d.ts",
      "import": "./src/components.js",
      "default": "./src/components.js"
    },
    "./constants": {
      "types": "./src/constants.d.ts",
      "import": "./src/constants.js",
      "default": "./src/constants.js"
    },
    "./utils": {
      "types": "./src/utils.d.ts",
      "import": "./src/utils.js",
      "default": "./src/utils.js"
    },
    "./dom": {
      "types": "./src/renderers/dom.d.ts",
      "import": "./src/renderers/dom.js",
      "default": "./src/renderers/dom.js"
    },
    "./html": {
      "types": "./src/renderers/html.d.ts",
      "import": "./src/renderers/html.js",
      "default": "./src/renderers/html.js"
    },
    "./reflow": {
      "types": "./src/renderers/reflow.d.ts",
      "import": "./src/renderers/reflow.js",
      "default": "./src/renderers/reflow.js"
    },
    "./jsx-runtime": {
      "types": "./src/renderers/jsx-runtime.d.ts",
      "import": "./src/renderers/jsx-runtime.js",
      "default": "./src/renderers/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./src/renderers/jsx-dev-runtime.d.ts",
      "import": "./src/renderers/jsx-dev-runtime.js",
      "default": "./src/renderers/jsx-dev-runtime.js"
    },
    "./browser": {
      "types": "./src/presets/browser.d.ts",
      "import": "./src/presets/browser.js",
      "default": "./src/presets/browser.js"
    },
    "./extras": {
      "types": "./src/extras/index.d.ts",
      "import": "./src/extras/index.js",
      "default": "./src/extras/index.js"
    },
    "./renderers/": "./src/renderers/",
    "./presets/": "./src/presets/",
    "./extras/": "./src/extras/",
    "./package.json": "./package.json"
  },
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SudoMaker/rEFui.git"
  },
  "author": "Yukino Song <yukino@sudomaker.com>",
  "license": "Apache-2.0",
  "bugs": "https://github.com/SudoMaker/rEFui/issues",
  "homepage": "https://github.com/SudoMaker/rEFui"
}
